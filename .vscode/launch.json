{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "repo-stats Debug",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "tsx",
      "program": "${workspaceFolder}/src/index.ts",
      "args": ["repo-stats"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
