{
  "scripts": {
    "all": "npm run lint:fix && npm run format:write && npm run build && npm run test:ci",
    "bundle": "npm run format:write && npm run package",
    "start": "node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --fix-dry-run",
    "package": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "package:watch": "npm run package -- --watch",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:file": "vitest run",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "build": "npm run bundle",
    "prepare-release": "./script/prepare-release.sh"
  },
  "name": "gh-repo-stats-plus",
  "version": "2.3.0",
  "main": "dist/index.js",
  "description": "GitHub CLI extension for gathering comprehensive repository statistics from GitHub organizations",
  "keywords": [
    "github",
    "cli",
    "extension",
    "statistics",
    "repository",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@octokit/openapi-types": "^27.0.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.9.1",
    "@types/shell-quote": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitest/coverage-v8": "^3.2.2",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "rollup": "^4.55.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@fast-csv/parse": "^5.0.5",
    "@octokit/graphql": "^9.0.3",
    "@octokit/plugin-paginate-graphql": "^6.0.0",
    "@octokit/plugin-throttling": "^11.0.3",
    "commander": "^14.0.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "octokit": "^5.0.5",
    "undici": "^7.21.0",
    "winston": "^3.19.0"
  }
}
